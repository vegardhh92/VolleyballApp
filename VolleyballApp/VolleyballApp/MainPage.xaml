<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:VolleyballApp"
             x:Class="VolleyballApp.MainPage"
             >

    <StackLayout Margin="10,10,10,0">
        <Button Text="New Game" Clicked="New_Game_Clicked"/>
        <Button Text="Archive" Clicked="Archive_Clicked"/>
        <Button Text="Team" Clicked="Team_Clicked"/>
        <Button Text="Test for statistics" Clicked="Statistics_Clicked"/>
    </StackLayout>

</ContentPage>
